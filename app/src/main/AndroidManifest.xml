<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="enafilipovic.ferit.summerbody">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.PhotoTrackerActivity"></activity>
        <activity android:name=".Activities.MeasurementTrackerActivity" />
        <activity android:name=".Activities.WeightHistoryActivity" />
        <activity android:name=".Activities.WeightTrackerActivity" />
        <activity android:name=".Activities.ProgramWorkoutActivity" />
        <activity android:name=".Activities.ProgramDetailsActivity" />
        <activity
            android:name=".Activities.ProgramViewActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>